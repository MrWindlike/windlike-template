{
  "extends": "eslint:recommended",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "react/prop-types": "warn",
    "semi": "error",
    "quotes": [ "error", "single"],
    "no-console": "off",
    "global-require": "off"
  },
  "globals": {
    "require": true
  },
  "env": {
    "es6": true,
    "browser": true
  }
}
